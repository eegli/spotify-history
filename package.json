{
  "name": "spotify-history-lambda",
  "version": "1.0.0",
  "private": true,
  "homepage": "https://github.com/eegli/spotify-history",
  "repository": "git@github.com:eegli/spotify-history.git",
  "license": "MIT",
  "author": "Eric <43848365+eegli@users.noreply.github.com>",
  "main": "build/index.js",
  "scripts": {
    "build": "tsc --build",
    "build:client": "tsc -p tsconfig.client.json",
    "dynamo": "sls dynamodb start --stage local",
    "dynamo:seed": "sls dynamodb start --seed=test --stage local",
    "format": "prettier --write src/**/*.{ts,js}",
    "postinstall": "husky install",
    "install:ci": "yarn install --ignore-scripts --ignore-optional --frozen-lockfile ",
    "lint": "eslint src --ext .js,.ts",
    "lint:watch": "esw -w --color src --ext .js,.ts",
    "local:backup": "sls invoke local -f spotify-history-backup -s local",
    "local:history": "sls invoke local -f spotify-history -s local",
    "prod:deploy": "sls deploy -s prod",
    "stg:__remove": "sls remove -s stg",
    "stg:deploy": "sls deploy -s stg",
    "stg:deploy:backup": "sls deploy -f spotify-history-backup -s stg",
    "stg:deploy:history": "sls deploy -f spotify-history -s stg",
    "stg:invoke:backup": "sls invoke -f spotify-history-backup -s stg",
    "stg:invoke:history": "sls invoke -f spotify-history -s stg",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:summary": "jest --coverage --coverageReporters=\"text-summary\"",
    "test:watch": "jest --watch",
    "token:google": "ts-node -T scripts/google.ts",
    "token:spotify": "ts-node -T scripts/spotify.ts"
  },
  "dependencies": {
    "@aws/dynamodb-data-mapper": "^0.7.3",
    "@aws/dynamodb-data-mapper-annotations": "^0.7.3",
    "@googleapis/drive": "^2.2.0",
    "axios": "^0.23.0",
    "moment": "^2.29.1"
  },
  "devDependencies": {
    "@googleapis/oauth2": "^0.2.0",
    "@types/aws-lambda": "^8.10.92",
    "@types/jest": "^27.4.0",
    "@types/mitm": "^1.3.4",
    "@types/node": "^16.11.13",
    "@types/spotify-api": "^0.0.11",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/parser": "^5.12.1",
    "aws-sdk": "^2.1052.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.9.0",
    "eslint-watch": "^7.0.0",
    "fork-ts-checker-webpack-plugin": "^6.5.0",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "lint-staged": "^11.2.6",
    "nodemon": "^2.0.13",
    "prettier": "^2.5.1",
    "prettier-plugin-organize-imports": "^2.3.4",
    "serverless-dynamodb-local": "0.2.40",
    "serverless-webpack": "^5.6.1",
    "spotify-auth-token": "^3.2.0",
    "ts-jest": "^27.1.3",
    "ts-loader": "^9.2.8",
    "ts-node": "^10.5.0",
    "typescript": "^4.5.5",
    "webpack": "^5.69.1",
    "webpack-node-externals": "^3.0.0"
  },
  "engines": {
    "node": ">=14.17.4"
  }
}
