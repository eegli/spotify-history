{
  "name": "spotify-history-lambda",
  "version": "1.0.0",
  "private": true,
  "main": "build/index.js",
  "engines": {
    "node": "12.16.1"
  },
  "repository": "git@github.com:eegli/spotify-history-lambda.git",
  "author": "Eric <43848365+eegli@users.noreply.github.com>",
  "license": "MIT",
  "scripts": {
    "dev": "ts-node -r dotenv/config src/index.ts",
    "build": "tsc --build",
    "build:client": "tsc -p tsconfig.client.json",
    "prod:spotify:deploy": "serverless deploy function -f spotify-history-lambda -s prod",
    "prod:deploy": "serverless deploy -s prod",
    "stg:spotify:deploy": "serverless deploy function -f spotify-history-lambda -s stg",
    "stg:deploy": "serverless deploy -s stg --force",
    "invoke:stg": "serverless invoke -f spotify-history-lambda -s stg",
    "invoke:local": "serverless invoke local -f spotify-history-lambda -s stg",
    "token": "yarn build:client && ts-node -r dotenv/config scripts/server.ts"
  },
  "dependencies": {
    "aws-sdk": "^2.878.0",
    "axios": "^0.21.1",
    "dotenv": "^8.2.0",
    "open": "^8.0.4",
    "typescript": "^4.2.3"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.72",
    "@types/aws-sdk": "^2.7.0",
    "@types/express": "^4.17.11",
    "@types/node": "^14.14.35",
    "@types/spotify-api": "^0.0.8",
    "express": "^4.17.1",
    "nanoid": "^3.1.22",
    "serverless-plugin-typescript": "^1.1.9",
    "ts-node": "^9.1.1"
  }
}
